{
  "name": "ns-ui-components",
  "version": "1.4.0",
  "description": "A typescript-react-based library for UI components",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "start-storybook -p 6006",
    "test": "jest --watchAll=false --verbose",
    "test:ci": "npm test -- --coverage",
    "lint": "eslint src/**/*.tsx",
    "prettier": "prettier src/**/*.{tsx,ts} --write",
    "lint-staged": "lint-staged -v",
    "prepare": "npm run build && husky install",
    "build": "rollup -c --bundleConfigAsCjs",
    "build:storybook": "build-storybook",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/naptune-sailor/ns-UI-components.git"
  },
  "keywords": [
    "typescript",
    "react",
    "rollup",
    "semantic-release",
    "jest",
    "prettier",
    "eslint",
    "husky",
    "component-library"
  ],
  "author": "Himanshu Savita",
  "license": "Naptunesailor",
  "homepage": "https://github.com/naptune-sailor/ns-UI-components#readme",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^9.0.2",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^8.3.0",
    "bulma": "^0.9.4",
    "cssnano": "^7.0.2",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "lint-staged": "^13.1.0",
    "node-sass": "^9.0.0",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.18.0",
    "rollup-plugin-dts": "^5.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.77.5",
    "sass-loader": "^14.2.1",
